{
  "name": "fb.net",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:turbo": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "migrate:news": "tsx scripts/migrate-news-to-supabase.ts",
    "migrate:news:postgres": "tsx scripts/migrate-news-to-postgres.ts",
    "migrate:images": "tsx scripts/migrate-images-to-storage.ts",
    "migrate:db-images": "tsx scripts/import-images-to-db.ts",
    "migrate:all-images-to-db": "tsx scripts/migrate-all-images-to-db.ts",
    "docker:migrate-db-images": "docker exec -i fb-net-nextjs npx tsx scripts/import-images-to-db.ts",
    "docker:migrate-all-images": "docker exec -i fb-net-nextjs npx tsx scripts/migrate-all-images-to-db.ts",
    "organize:images": "tsx scripts/organize-images-by-date.ts",
    "rename:folders": "tsx scripts/rename-date-folders.ts",
    "create:news-from-folders": "tsx scripts/create-news-from-folders.ts",
    "setup": "tsx scripts/setup.ts",
    "setup:supabase": "supabase init && supabase start",
    "setup:supabase:install": "npm install -g supabase",
    "check:docker": "docker ps",
    "check:supabase": "supabase --version",
    "setup:docker": "docker-compose -f docker-compose.simple.yml up -d && npm run docker:init-db",
    "docker:up": "docker-compose -f docker-compose.simple.yml up -d --build",
    "docker:down": "docker-compose -f docker-compose.simple.yml down",
    "docker:logs": "docker-compose -f docker-compose.simple.yml logs -f",
    "docker:rebuild": "docker-compose -f docker-compose.simple.yml up -d --build --force-recreate",
    "docker:psql": "docker exec -it fb-net-supabase-db psql -U postgres -d postgres",
    "docker:init-db": "docker exec -i fb-net-supabase-db psql -U postgres -d postgres < supabase-schema.sql",
    "docker:fix-tables": "docker exec -i fb-net-supabase-db psql -U postgres -d postgres < fix-missing-tables.sql",
    "docker:check-tables": "docker exec -i fb-net-supabase-db psql -U postgres -d postgres -c \"\\dt\"",
    "docker:prod": "docker-compose -f docker-compose.prod.yml up -d --build",
    "fix:image-paths": "tsx scripts/fix-image-paths.ts",
    "docker:add-views-tracking": "docker exec -i fb-net-supabase-db psql -U postgres -d postgres < scripts/add-views-tracking.sql",
    "setup:telegram-bot": "tsx scripts/setup-telegram-bot.ts",
    "copy:trainings": "tsx scripts/copy-trainings-images.ts",
    "copy:trainings-to-blog": "tsx scripts/copy-trainings-to-blog.ts",
    "merge:blog-folders": "tsx scripts/merge-consecutive-blog-folders.ts",
    "cleanup:blog-folders": "tsx scripts/cleanup-duplicate-blog-folders.ts"
  },
  "dependencies": {
    "@pbe/react-yandex-maps": "^1.2.5",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/supabase-js": "^2.79.0",
    "axios": "^1.13.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "exifr": "^7.1.3",
    "framer-motion": "^12.23.24",
    "jose": "^6.1.3",
    "lucide-react": "^0.546.0",
    "next": "15.5.6",
    "node-telegram-bot-api": "^0.66.0",
    "nodemailer": "^6.9.16",
    "pg": "^8.16.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20.19.24",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/nodemailer": "^6.4.17",
    "@types/pg": "^8.15.6",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "15.5.6",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
