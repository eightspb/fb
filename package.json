{
  "name": "fb.net",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:no-turbo": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:ci": "bun run type-check && bun run lint && bun run test:unit && bun run test:e2e",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:psql": "docker exec -it fb-net-postgres psql -U postgres -d postgres",
    "docker:prod": "docker-compose -f docker-compose.production.yml up -d --build",
    "docker:test": "docker-compose -f docker-compose.test.yml up --build --abort-on-container-exit",
    "docker:test:down": "docker-compose -f docker-compose.test.yml down -v",
    "bun:install": "bun install",
    "bun:update": "bun update",
    "bun:check": "bun pm ls"
  },
  "dependencies": {
    "@pbe/react-yandex-maps": "^1.2.5",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.2",
    "axios": "^1.13.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "exifr": "^7.1.3",
    "form-data": "^4.0.0",
    "framer-motion": "^12.31.0",
    "jose": "^6.1.3",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "node-telegram-bot-api": "^0.67.0",
    "nodemailer": "^8.0.0",
    "pg": "^8.18.0",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@testcontainers/postgresql": "^11.10.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^25.2.0",
    "@types/node-telegram-bot-api": "^0.64.13",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.16.0",
    "@types/react": "^19.2.11",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "jsdom": "^25.0.1",
    "msw": "^2.6.5",
    "tailwindcss": "^4.1.18",
    "testcontainers": "^11.10.0",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
